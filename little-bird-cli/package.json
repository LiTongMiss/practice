    "babel-cli": "^6.26.0",
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "download-git-repo": "^3.0.2",
    "inquirer": "^8.0.0",
    "log-symbols": "^4.0.0",
    "open-browser-webpack-plugin": "0.0.5",
    "ora": "^5.3.0",
    "webpack": "^5.25.0",
    "webpack-dev-server": "^3.11.2"
  }
}
